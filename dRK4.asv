function dx = dRK4(x, t, h)

f = @L96;
df = @dL96;

dx = eye(length(x));

x = RK4(f, t, x, h);

for i = 1:length(t)-1
    k1 = f(t(i), x(:,i));
    dk1 = df(x(:,i))*dx;
    k2 = f(x(:,i) + 1/2*h*k1);
    dk2 = df(x(:,i) + 1/2*h*k1) * (dx + 1/2*h*dk1);
    k3 = f(x(:,i) + 1/2*h*k2);
    dk3 = df(x(:,i) +
    k4 = f(t(i) + h, x(:,i) + h*k3);
    
    x(:,i+1) = x(:,i) + 1/3*h*(1/2*k1 + k2 + k3 + 1/2*k4);
end

end